{
  "name": "dotenv-sebas",
  "version": "0.1.0",
  "description": "A template for creating npm packages using TypeScript and VSCode",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "cm": "cz",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepare": "is-ci || husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kainstar/dotenv-sebas.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kainstar",
    "email": "15249633+kainstar@users.noreply.github.com",
    "url": "https://github.com/kainstar"
  },
  "engines": {
    "node": ">=14.0"
  },
  "keywords": [
    "boilerplate",
    "template",
    "typescript",
    "vscode",
    "semantic-release"
  ],
  "bugs": {
    "url": "https://github.com/kainstar/dotenv-sebas/issues"
  },
  "homepage": "https://github.com/kainstar/dotenv-sebas#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dotenv": "^16.0.3"
  }
}